<h1>Welcome to the SLOW-DRONGO bookstore</h1>

<div class="books">
  <ul style="list-style-type:none">

    {{#each books}}
      <li>{{title}}</li>
      <li>{{description}}</li>
      <li><img src="{{img_url}}" width="400px" height="300px" alt="Image" /></li>
    {{else}}
      <h1>Not Found</h1>
    {{/each}}

  </ul>
</div>
